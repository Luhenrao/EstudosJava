import java.util.ArrayList;

public class main {
    public static void main(String[] args) {
        ArrayList<String> mylist = new ArrayList<>();
        String n1 = "Gilberto";
        String n2 = "Roberval";
        String n3 = "Juslelsson";
        mylist.add(n1);
        mylist.add(n2);
        mylist.add(n3);


            mylist.remove(1); //f gilberto

            exibirArr(mylist);
    }
    public static void exibirArr(ArrayList<String> lista){
        for(int i= 0; i<lista.size(); i++){
            System.out.println(lista);
        }
        
    }
}
